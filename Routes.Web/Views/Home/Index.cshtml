@model IEnumerable<Routes.Dal.Entities.Route>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RouteEnterTupe)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TravelType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OriginPoint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DestinationPoint)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RouteEnterTupe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TravelType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OriginPoint)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DestinationPoint)
        </td>
        <td>
            @Html.ActionLink("Edit", "ShowRoute","Route", new { id=item.RouteID },null) |
            @Html.ActionLink("Details", "Details", new { id=item.RouteID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RouteID })
        </td>
    </tr>
}

</table>
